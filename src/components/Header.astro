---
const LINKS_NAVBAR = [{ title: "Sobre el proyecto" }];
---

<header class="px-24 py-2 flex gap-x-24">
  <h1
    class="text-2xl font-bold max-md:text-center text-yellow-400 hover:text-yellow-200 transition-all duration-300"
  >
    <a href="/">FinalBoss</a>
  </h1>
  <ul class="w-full">
    {
      LINKS_NAVBAR.map((link) => {
        <li>
          <a class="text-white" href="/">
            {link.title}
          </a>
        </li>;
      })
    }
  </ul>
</header>
